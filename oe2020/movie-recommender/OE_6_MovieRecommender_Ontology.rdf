    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Action -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Action">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Genre"/>
        <rdfs:label>Action</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Actor -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Actor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Contributor"/>
        <definition>person who plays an on screen role in a movie, show, or series</definition>
        <rdfs:isDefinedBy>“schema.org,” Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
        <rdfs:label>Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#AgeRating -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#AgeRating">
        <definition>classification, applied to products, specifying a minimum age for a person using it.</definition>
        <rdfs:isDefinedBy>&quot;age rating&quot;, Your-Dictionary. [Online]. Available: https://www.yourdictionary.com/age-rating. [Accessed: 16-Oct-2020]</rdfs:isDefinedBy>
        <rdfs:label>AgeRating</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#AnimatedMovie -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#AnimatedMovie">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Style"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#LiveActionMovie"/>
        <definition>&quot;animated&quot;, Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/animated. [Accessed: 2-Oct-2020]</definition>
        <definition>film made in the form of a cartoon</definition>
        <rdfs:label>AnimatedMovie</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#ArtFilm -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#ArtFilm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#ProductionType"/>
        <skoks:definition>classification of a film indicating it is meant to be a serious artistic work</skoks:definition>
        <rdfs:isDefinedBy>T. Dirks, “ Film Sub-Genres Types (and Hybrids),” Film Sub-Genres Types (and Hybrids). [Online]. Available: https://www.filmsite.org/subgenres2.html. [Accessed: 03-Oct-2020].</rdfs:isDefinedBy>
        <rdfs:label>ArtFilm</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Author -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Author">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Contributor"/>
        <definition>author of the original content for which the work is based on</definition>
        <rdfs:isDefinedBy>“schema.org,” Contributor Schema. [Online]. Available: https://schema.org/contributor. [Accessed: 2-Oct-2020].</rdfs:isDefinedBy>
        <rdfs:label>Author</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Award -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#Award">
        <definition>something that recognizes a movie or show for a quality</definition>
        <rdfs:isDefinedBy>“schema.org,” Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
        <rdfs:label>Award</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#CinematicUniverse -->

    <owl:Class rdf:about="http://www.semanticweb.org/justi/ontologies/2020/9/MovieRecommenderV1#CinematicUniverse">
        <definition>set of creative works where more than one writer independently contributes a work that can stand alone but fits into the joint development of the storyline, characters, or world of the overall project</definition>
        <rdfs:isDefinedBy>&quot;shared universe&quot;, Wikipedia. [Online]. Available: https://en.wikipedia.org/wiki/Shared_universe. [Accessed: 16-Oct-2020]</rdfs:isDefinedBy>
