<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fnd-aap-ppl "https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/">
	<!ENTITY fibo-fnd-arr-cls "https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/">
	<!ENTITY fibo-fnd-arr-doc "https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/">
	<!ENTITY fibo-fnd-arr-id "https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/">
	<!ENTITY fibo-fnd-arr-rt "https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/">
	<!ENTITY fibo-fnd-dt-bd "https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/BusinessDates/">
	<!ENTITY fibo-fnd-dt-fd "https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-dt-oc "https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/Occurrences/">
	<!ENTITY fibo-fnd-org-fm "https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/">
	<!ENTITY fibo-fnd-pas-pas "https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/">
	<!ENTITY fibo-fnd-plc-loc "https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/">
	<!ENTITY fibo-fnd-plc-vrt "https://spec.edmcouncil.org/fibo/ontology/FND/Places/VirtualPlaces/">
	<!ENTITY fibo-fnd-pty-pty "https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/">
	<!ENTITY fibo-fnd-pty-rl "https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/">
	<!ENTITY fibo-fnd-rel-rel "https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-alx "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/">
	<!ENTITY fibo-fnd-utl-av "https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY lcc-cr "https://www.omg.org/spec/LCC/Countries/CountryRepresentation/">
	<!ENTITY lcc-lr "https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/">
	<!ENTITY oe2020-mov-rec "https://tw.rpi.edu/ontology-engineering/oe2020/movie-recommender/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://tw.rpi.edu/ontology-engineering/oe2020/movie-recommender/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fnd-aap-ppl="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/"
	xmlns:fibo-fnd-arr-cls="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/ClassificationSchemes/"
	xmlns:fibo-fnd-arr-doc="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/"
	xmlns:fibo-fnd-arr-id="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/"
	xmlns:fibo-fnd-arr-rt="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/"
	xmlns:fibo-fnd-dt-bd="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/BusinessDates/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-dt-oc="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/Occurrences/"
	xmlns:fibo-fnd-org-fm="https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/"
	xmlns:fibo-fnd-pas-pas="https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/"
	xmlns:fibo-fnd-plc-loc="https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/"
	xmlns:fibo-fnd-plc-vrt="https://spec.edmcouncil.org/fibo/ontology/FND/Places/VirtualPlaces/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/"
	xmlns:fibo-fnd-pty-rl="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-alx="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:lcc-cr="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"
	xmlns:lcc-lr="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"
	xmlns:oe2020-mov-rec="https://tw.rpi.edu/ontology-engineering/oe2020/movie-recommender/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://tw.rpi.edu/ontology-engineering/oe2020/movie-recommender/">
		<rdfs:label>Movie Recommender Ontology</rdfs:label>
		<dct:abstract>This ontology ...</dct:abstract>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/ontology/FND/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://www.omg.org/spec/LCC/</sm:dependsOn>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/IdentifiersAndIndices/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/ClassificationSchemes/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/BusinessDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/Occurrences/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Roles/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Places/VirtualPlaces/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
		<owl:versionIRI rdf:resource="https://tw.rpi.edu/ontology-engineering/oe2020/20201003/movie-recommender/"/>
		<skos:changeNote>The https://tw.rpi.edu/ontology-engineering/oe2020/20201003/movie-recommender.rdf version of this ontology is the seed version.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Artist">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Contributor"/>
		<rdfs:label>artist</rdfs:label>
		<skos:definition>contributor who practices or is proficient in a fine or performing art</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.thefreedictionary.com/artist</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Character">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:label>character</rdfs:label>
		<skos:definition>fictional person, or the portrayal of a real person in a movie or show</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020]</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;CinematicUniverse">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;SharedUniverse"/>
		<rdfs:label>cinematic universe</rdfs:label>
		<skos:definition>shared universe that incorporates some corporate advertisement and marketing strategy for cross-selling multiple IP franchises to a larger combined audience</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.reddit.com/r/RedLetterMedia/comments/8abtlo/the_practical_definition_of_a_cinematic_universe/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Contributor">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;Actor"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;Person"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>contributor</rdfs:label>
		<skos:definition>someone who contributes to a creative work</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://schema.org/contributor</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>Note that this use of the term, contributor is distinct from its use in schema.org, where contributors do not include the primary creator.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;CreativeWork">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;isDerivedFrom"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;CreativeWork"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasRepresentation"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;Expression"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasContributor"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;Contributor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>creative work</rdfs:label>
		<skos:definition>manifestation of creative effort including fine artwork (sculpture, paintings, drawing, sketching, performance art), dance, writing (literature), filmmaking, and composition</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Creative_work</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Expression">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-arr-rt;hasRating"/>
				<owl:onClass rdf:resource="&fibo-fnd-arr-rt;Rating"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasSetting"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;Setting"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasInitialReleaseDate"/>
				<owl:onClass rdf:resource="&fibo-fnd-dt-fd;ExplicitDate"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;represents"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;CreativeWork"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasContributor"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;Contributor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>expression</rdfs:label>
		<skos:definition>manifestation of a specific version of a creative work</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote>Many films and television programs have been remade over time in various forms.  An expression represents a version of a given creative work. It may be filmed in some time frame with a specific set of contributors and in a specific format (e.g., animated film vs. full-feature film with live actors).  Note that there may be multiple versions of the same expression, for example, the same film re-recorded with voice over in a different natural language. In that case, the same character may be performed by multiple actors - the original actor on screen and a different actor for the voice.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Setting">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-plc-loc;Location"/>
		<rdfs:label>setting</rdfs:label>
		<skos:definition>particular place or type of surroundings where something is or takes place</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.collinsdictionary.com/dictionary/english/setting</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;SharedUniverse">
		<rdfs:subClassOf rdf:resource="&lcc-lr;Collection"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;comprises"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;CreativeWork"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>shared universe</rdfs:label>
		<skos:definition>set of creative works where more than one writer (or other artist) independently contributes a work that can stand alone but fits into the joint development of the storyline, characters, or world of the overall project</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>https://en.wikipedia.org/wiki/Shared_universe</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;contributesTo">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
		<rdfs:label>is derived from</rdfs:label>
		<owl:inverseOf rdf:resource="&oe2020-mov-rec;hasDerivation"/>
		<skos:definition>is developed from</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote>Note that any creative work or expression of that work might be derived from or adapted from something else, such as a book or prior expression of a creative work.</fibo-fnd-utl-av:explanatoryNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;hasContributor">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-pty-pty;hasActor"/>
		<rdfs:label>has contributor</rdfs:label>
		<skos:definition>links a party to something that they have contributed to</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;hasDerivation">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
		<rdfs:label>has derivation</rdfs:label>
		<skos:definition>is a source for</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;hasInitialReleaseDate">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-dt-fd;hasExplicitDate"/>
		<rdfs:label>has initial release date</rdfs:label>
		<skos:definition>date on which something is first published; a date fixed in advance for the release of a film, recording, or similar product</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.lexico.com/definition/release_date</fibo-fnd-utl-av:adaptedFrom>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;hasSetting">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-plc-loc;isLocatedAt"/>
		<rdfs:label>has setting</rdfs:label>
		<skos:definition>indicates the place or environment where something takes place</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-mov-rec;isDerivedFrom">
		<rdfs:subPropertyOf rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
		<rdfs:label>is derived from</rdfs:label>
		<owl:inverseOf rdf:resource="&oe2020-mov-rec;hasDerivation"/>
		<skos:definition>is developed from</skos:definition>
		<fibo-fnd-utl-av:explanatoryNote>Note that any creative work or expression of that work might be derived from or adapted from something else, such as a book or prior expression of a creative work.</fibo-fnd-utl-av:explanatoryNote>
	</owl:ObjectProperty>

</rdf:RDF>