	<owl:Class rdf:about="&fibo-fnd-plc-loc;Location">
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;3DAnimatedMovie">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;AnimatedMovie"/>
		<rdfs:label>3D animated movie</rdfs:label>
		<rdfs:isDefinedBy>&quot;The 5 Types of Animation&quot;, Bloop. [Online]. Available: https://www.bloopanimation.com/types-of-animation/. [Accessed 15-Oct-2020].</rdfs:isDefinedBy>
		<skos:definition>animators design objects and movement paths, and the computer interpolates the frames of animation between the key frames</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Action">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Genre"/>
		<rdfs:label>action</rdfs:label>
		<skos:definition>film genre in which the protagonist is thrust into a series of events that typically involve violence or physical feats</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;AgeRating">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-arr-rt;Rating"/>
		<rdfs:label>age rating</rdfs:label>
		<rdfs:isDefinedBy>&quot;age rating&quot;, Your-Dictionary. [Online]. Available: https://www.yourdictionary.com/age-rating. [Accessed: 16-Oct-2020]</rdfs:isDefinedBy>
		<skos:definition>classification, applied to products, specifying a minimum age for a person using it.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;AnimatedMovie">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Style"/>
		<rdfs:label>animated movie</rdfs:label>
		<rdfs:isDefinedBy>&quot;animated&quot;, Merriam-Webster. [Online]. Available: https://www.merriam-webster.com/dictionary/animated. [Accessed: 2-Oct-2020]</rdfs:isDefinedBy>
		<owl:disjointWith rdf:resource="&oe2020-mov-rec;LiveActionMovie"/>
		<skos:definition>film made in the form of a cartoon</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;ArtFilm">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;ProductionType"/>
		<rdfs:label>art film</rdfs:label>
		<rdfs:isDefinedBy>T. Dirks, &apos; Film Sub-Genres Types (and Hybrids),&apos; Film Sub-Genres Types (and Hybrids). [Online]. Available: https://www.filmsite.org/subgenres2.html. [Accessed: 03-Oct-2020].</rdfs:isDefinedBy>
		<skos:definition>classification of a film indicating it is meant to be a serious artistic work</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Artist">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Contributor"/>
		<rdfs:label>artist</rdfs:label>
		<skos:definition>contributor who practices or is proficient in a fine or performing art</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.thefreedictionary.com/artist</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Author">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Contributor"/>
		<rdfs:label>author</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; Contributor Schema. [Online]. Available: https://schema.org/contributor. [Accessed: 2-Oct-2020].</rdfs:isDefinedBy>
		<skos:definition>author of the original content for which the work is based on</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&oe2020-mov-rec;AvatarTheLastAirbender">
		<rdf:type rdf:resource="&oe2020-mov-rec;TVShow"/>
		<rdfs:label>Avatar The Last Airbender</rdfs:label>
		<rdfs:isDefinedBy>&quot;Avatar: The Last Airbender&quot;, IMDB. [Online]. Available: https://www.imdb.com/title/tt0417299/?ref_=nv_sr_srsg_0. [Accessed 23-Sep-2020]</rdfs:isDefinedBy>
		<skos:definition>television series about a young boy who reawakens after a 100-year long slumber and undergoes a quest to fulfill his destiny as the master of the 4 elements</skos:definition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Award">
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasAwardProductionType"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;ProductionType"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>award</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
		<skos:definition>something that recognizes a movie or show for a quality</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&oe2020-mov-rec;BradBird">
		<rdf:type rdf:resource="&oe2020-mov-rec;Director"/>
		<rdf:type rdf:resource="&oe2020-mov-rec;Writer"/>
		<rdfs:label>Brad Bird</rdfs:label>
		<rdfs:isDefinedBy>&quot;Brad Bird&quot;, IMDB. [Online]. Available: https://www.imdb.com/name/nm0083348/?ref_=nv_sr_srsg_0. [Accessed 23-Sep-2020]</rdfs:isDefinedBy>
		<skos:definition>american director known for both his animated and live action world</skos:definition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Character">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:label>character</rdfs:label>
		<skos:definition>fictional person, or the portrayal of a real person in a movie or show</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020]</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;CinematicUniverse">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;SharedUniverse"/>
		<rdfs:label>cinematic universe</rdfs:label>
		<rdfs:isDefinedBy>&quot;shared universe&quot;, Wikipedia. [Online]. Available: https://en.wikipedia.org/wiki/Shared_universe. [Accessed: 16-Oct-2020]</rdfs:isDefinedBy>
		<skos:definition>shared universe that incorporates some corporate advertisement and marketing strategy for cross-selling multiple IP franchises to a larger combined audience</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.reddit.com/r/RedLetterMedia/comments/8abtlo/the_practical_definition_of_a_cinematic_universe/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Comedian">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;Actor"/>
		<rdfs:label>comedian</rdfs:label>
		<skos:definition>an entertainer whose act is designed to make an audience laugh</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Comedy">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Genre"/>
		<rdfs:label>comedy</rdfs:label>
		<skos:definition>category of film with an emphasis on humor</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&oe2020-mov-rec;Community">
		<rdf:type rdf:resource="&oe2020-mov-rec;TVShow"/>
		<rdfs:label>Community</rdfs:label>
		<rdfs:isDefinedBy>&quot;Community&quot;, IMDB. [Online]. Available: https://www.imdb.com/title/tt1439629/?ref_=nv_sr_srsg_0. [Accessed 23-Sep-2020]</rdfs:isDefinedBy>
		<skos:definition>television series about a suspended lawyer forced to enroll in a community college with an eccentric staff and student body</skos:definition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Contributor">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;Actor"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;Person"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;bornIn"/>
				<owl:onClass rdf:resource="&lcc-lr;Location"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>contributor</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
		<skos:definition>someone who contributes to a creative work</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/contributor [Accessed: 23-Sep-2020].</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>Note that this use of the term, contributor is distinct from its use in schema.org, where contributors do not include the primary creator.</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;CreativeWork">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasAgeRating"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;AgeRating"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;isDerivedFrom"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;CreativeWork"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasRepresentation"/>
				<owl:onClass rdf:resource="&oe2020-mov-rec;Expression"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasContributor"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;Contributor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasProductAward"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;ProductAward"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-mov-rec;hasProductionType"/>
				<owl:someValuesFrom rdf:resource="&oe2020-mov-rec;ProductionType"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>creative work</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
		<skos:definition>manifestation of creative effort including fine artwork (sculpture, paintings, drawing, sketching, performance art), dance, writing (literature), filmmaking, and composition</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Creative_work</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Creator">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Contributor"/>
		<rdfs:label>creator</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; TV Series Schema. [Online]. Available: https://schema.org/TVSeries. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
		<skos:definition>creator for this work</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Director">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Contributor"/>
		<rdfs:label>director</rdfs:label>
		<rdfs:isDefinedBy>&apos;schema.org,&apos; Movie Schema. [Online]. Available: https://schema.org/Movie. [Accessed: 23-Sep-2020].</rdfs:isDefinedBy>
		<skos:definition>person who directs the filming of a movie, show, or series</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Drama">
		<rdfs:subClassOf rdf:resource="&oe2020-mov-rec;Genre"/>
		<rdfs:label>drama</rdfs:label>
		<skos:definition>category of film intended to be more serious than humorous in tone</skos:definition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&oe2020-mov-rec;Emmy">
		<rdf:type rdf:resource="&oe2020-mov-rec;Award"/>
		<rdfs:label>Emmy</rdfs:label>
		<rdfs:isDefinedBy>&quot;Emmys&quot;, IMDB. [Online]. Available: https://www.imdb.com/emmys/?ref_=nv_ev_rte. [Accessed 23-Sep-2020]</rdfs:isDefinedBy>
		<skos:definition>yearly set of awards given to particularly recognized television series for the year</skos:definition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&oe2020-mov-rec;Expression">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:subClassOf>
