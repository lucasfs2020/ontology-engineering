	<owl:Class rdf:about="&oe2020-beer;Bar">
		<rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
		<rdfs:label>bar</rdfs:label>
		<skos:definition>establishment where drinks, especially alcoholic drinks, are sold and consumed</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://dictionary.cambridge.org/dictionary/english/bar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beer">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Libation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isProducedBy"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Brewery"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>beer</rdfs:label>
		<skos:definition>beverage, often alcoholic, obtained from the fermentation of cereal grains</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>T. Burnham, J. Herz, J. Holl, C. Jones, M. Storey, L. Trautwein, A. Sparhawk, G. Puza, N. Zander, &apos;Beer Styles Study Guide&apos; CraftBeer.com, 2020. [Online]. Available: https://www.craftbeer.com/beer/beer-styles-guide. [Accessed Sept. 23, 2020]</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/us/definition/english/beer?q=beer</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beverage">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:label>beverage</rdfs:label>
		<skos:definition>consumable liquid</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/beverage</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Ale">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>ale</rdfs:label>
		<skos:definition>type of beer brewed using warm fermentation method</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Lager">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>lager</rdfs:label>
		<skos:definition>a type of beer conditioned at low temperature</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Lager</fibo-fnd-utl-av:adaptedFrom> 
	</owl:Class>
	
