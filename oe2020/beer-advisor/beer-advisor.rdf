		<rdfs:subClassOf rdf:resource="&oe2020-beer;Bar"/>
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Brewery"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-pas-pas;sells"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Beer"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">brewpub</rdfs:label>
		<skos:definition>establishment that sells beverages brewed on the premises and often includes a restaurant</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.dictionary.com/browse/brewpub</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/brewpub</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
