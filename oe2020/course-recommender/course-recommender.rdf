	<owl:Class rdf:about="&oe2020-crs-rec;AcademicSchedule">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:label>academic schedule</rdfs:label>
		<skos:definition>list of events and deadlines of importance to students covering the period of the university&apos;s academic year</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://info.rpi.edu/registrar/academic-calendar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:NamedIndividual rdf:about="&oe2020-crs-rec;AcademicYear">
		<rdf:type rdf:resource="&fibo-fnd-dt-fd;CalendarPeriod"/>
		<rdfs:label>academic year</rdfs:label>
		<skos:definition>calendar period during which students attend school or university, typically starting at the end of August and concluding near the end of May the following calendar year</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Academic_year</fibo-fnd-utl-av:adaptedFrom>
	</owl:NamedIndividual>

	<owl:Class rdf:about="&oe2020-crs-rec;Advisor">
		<rdfs:subClassOf rdf:resource="&oe2020-crs-rec;FacultyMember"/>
		<rdfs:label>advisor</rdfs:label>
		<skos:definition>faculty member tasked to work with students to develop educational plans, explain requirements and rules surrounding graduation, discuss and monitor academic progress, and provide general guidance</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://info.rpi.edu/advising-learning-assistance/advising/#FacultyStaffAdvisors</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:Class rdf:about="&oe2020-crs-rec;EducationalInstitution">
		<rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
		<rdfs:subClassOf rdf:resource="&fibo-be-oac-exec;AuthorizingParty"/>
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;ServiceProvider"/>
		<rdfs:label>educational institution</rdfs:label>
		<skos:definition>institution where people gain an education, providing a variety of learning environments and spaces</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://definitions.uslegal.com/e/educational-institution/</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.thefreedictionary.com/educational+institution</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote>According to 20 USCS section 1681 educational institution means &apos;any public or private preschool, elementary, or secondary school, or any institution of vocational, professional, or higher education, except that in the case of an educational institution composed of more than one school, college, or department which are administratively separate units, such term means each such school, college, or department.&apos;</fibo-fnd-utl-av:explanatoryNote>
	</owl:Class>

	<owl:Class rdf:about="&oe2020-crs-rec;FacultyMember">
		<rdfs:subClassOf rdf:resource="&fibo-be-oac-exec;AuthorizedParty"/>
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;OrganizationMember"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&lcc-lr;isMemberOf"/>
						<owl:someValuesFrom>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&fibo-fnd-pty-rl;playsRole"/>
								<owl:someValuesFrom rdf:resource="&oe2020-crs-rec;University"/>
							</owl:Restriction>
						</owl:someValuesFrom>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>faculty member</rdfs:label>
		<skos:definition>party employed by a college or district to conduct classroom or teaching activities or who is otherwise considered by the college to be a member of faculty</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.lawinsider.com/dictionary/faculty-member</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:Class rdf:about="&oe2020-crs-rec;Professor">
		<rdfs:subClassOf rdf:resource="&oe2020-crs-rec;FacultyMember"/>
		<rdfs:label>professor</rdfs:label>
		<skos:definition>teacher of the highest rank in a college or university</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Professor</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:NamedIndividual rdf:about="&oe2020-crs-rec;RensselaerPolytechnicInstitute">
		<rdf:type rdf:resource="&oe2020-crs-rec;University"/>
		<rdfs:label>Rensselaer Polytechnic Institute</rdfs:label>
		<skos:definition>globally recognized university situated on 275 acres in Troy NY, offering 746 labs, studios, and technology spaces used for research</skos:definition>
		<fibo-fnd-rel-rel:hasIdentity rdf:resource="&oe2020-crs-rec;RensselaerPolytechnicInstituteAsLegalEntity"/>
		<fibo-fnd-utl-av:abbreviation>RPI</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:abbreviation>Rensselaer</fibo-fnd-utl-av:abbreviation>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://rpi.edu/about/</fibo-fnd-utl-av:adaptedFrom>
	</owl:NamedIndividual>

	<owl:NamedIndividual rdf:about="&oe2020-crs-rec;RensselaerPolytechnicInstituteAddress">
		<rdf:type rdf:resource="&fibo-fnd-plc-adr;PhysicalAddress"/>
		<rdfs:label>Rensselaer Polytechnic Institute address</rdfs:label>
		<skos:definition>physical street address for Rensselaer Polytechnic Institute</skos:definition>
		<fibo-fnd-plc-adr:hasAddressLine1>110 8th Street</fibo-fnd-plc-adr:hasAddressLine1>
		<fibo-fnd-plc-adr:hasPostalCode>12180-3522</fibo-fnd-plc-adr:hasPostalCode>
		<fibo-fnd-plc-loc:hasCityName>Troy</fibo-fnd-plc-loc:hasCityName>
		<fibo-fnd-plc-loc:hasCountry rdf:resource="&lcc-3166-1;UnitedStates"/>
		<fibo-fnd-plc-loc:hasSubdivision rdf:resource="&lcc-3166-2-us;NewYork"/>
	</owl:NamedIndividual>

	<owl:NamedIndividual rdf:about="&oe2020-crs-rec;RensselaerPolytechnicInstituteAsLegalEntity">
		<rdf:type rdf:resource="&fibo-be-le-lp;LegalEntity"/>
		<rdfs:label>Rensselaer Polytechnic Institute as legal entity</rdfs:label>
		<skos:definition>legal entity that is Rensselaer Polytechnic Institute</skos:definition>
		<fibo-be-le-fbo:hasHeadquartersAddress rdf:resource="&oe2020-crs-rec;RensselaerPolytechnicInstituteAddress"/>
	</owl:NamedIndividual>

	<owl:NamedIndividual rdf:about="&oe2020-crs-rec;Semester">
		<rdf:type rdf:resource="&fibo-fnd-dt-fd;CalendarPeriod"/>
		<rdfs:label>semester</rdfs:label>
		<skos:definition>18-week period of instruction into which an academic year is often divided</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/semester</fibo-fnd-utl-av:adaptedFrom>
	</owl:NamedIndividual>

	<owl:Class rdf:about="&oe2020-crs-rec;Student">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
				<owl:onClass rdf:resource="&fibo-fnd-aap-ppl;Person"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-ra;isRegisteredBy"/>
				<owl:someValuesFrom rdf:resource="&fibo-fbc-fct-ra;Registrar"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>student</rdfs:label>
		<skos:definition>person who is formally registered at an educational institution to partake in academic courses with the purpose of acquiring a degree</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://catalog.rpi.edu/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:Class rdf:about="&oe2020-crs-rec;University">
		<rdfs:subClassOf rdf:resource="&oe2020-crs-rec;EducationalInstitution"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&lcc-lr;hasMember"/>
						<owl:someValuesFrom>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&fibo-fnd-pty-rl;playsRole"/>
								<owl:someValuesFrom rdf:resource="&oe2020-crs-rec;FacultyMember"/>
							</owl:Restriction>
						</owl:someValuesFrom>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>university</rdfs:label>
		<skos:definition>educational institution designed for instruction, examination, or both, of students in many branches of advanced learning, conferring degrees in various faculties, and often embodying colleges and similar institutions</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.lexico.com/en/definition/university</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

</rdf:RDF>