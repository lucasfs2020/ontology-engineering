oe2020-crs-rec:AcademicSchedule
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:Schedule ;
	rdfs:label "academic schedule" ;
	skos:definition "list of events and deadlines of importance to students covering the period of the university's academic year" ;
	fibo-fnd-utl-av:adaptedFrom "https://info.rpi.edu/registrar/academic-calendar"^^xsd:anyURI ;
	.

oe2020-crs-rec:AcademicYear
	a
		owl:NamedIndividual ,
		fibo-fnd-dt-fd:CalendarPeriod
		;
	rdfs:label "academic year" ;
	skos:definition "calendar period during which students attend school or university, typically starting at the end of August and concluding near the end of May the following calendar year" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Academic_year"^^xsd:anyURI ;
	.

oe2020-crs-rec:Advisor
	a owl:Class ;
	rdfs:subClassOf
		oe2020-crs-rec:FacultyMember ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:advises ;
			owl:someValuesFrom oe2020-crs-rec:Student ;
		]
		;
	rdfs:label "advisor" ;
	skos:definition "faculty member tasked to work with students to develop educational plans, explain requirements and rules surrounding graduation, discuss and monitor academic progress, and provide general guidance" ;
	fibo-fnd-utl-av:adaptedFrom "https://info.rpi.edu/advising-learning-assistance/advising/#FacultyStaffAdvisors"^^xsd:anyURI ;
	.

oe2020-crs-rec:Course
	a owl:Class ;
	rdfs:subClassOf
		owl:Thing ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasTopic> ;
			owl:onClass oe2020-crs-rec:TopicArea ;
			owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasCourseCode> ;
			owl:onClass oe2020-crs-rec:CourseCode ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasCredits ;
			owl:someValuesFrom xsd:int ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasDepartment> ;
			owl:someValuesFrom oe2020-crs-rec:Department ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasScheduledOffering> ;
			owl:someValuesFrom oe2020-crs-rec:SemesterSchedule ;
		]
		;
	rdfs:label "course" ;
	skos:definition "unit of teaching, usually an individual subject, that typically lasts one academic semester, is led by one or more instructors, and has a fixed roster of students" ;
	.

oe2020-crs-rec:CourseCode
	a owl:Class ;
	rdfs:subClassOf
		owl:Thing ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasCourseCode> ;
			owl:onClass oe2020-crs-rec:CourseCode ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasLevel ;
			owl:someValuesFrom xsd:int ;
		]
		;
	rdfs:label "course code" ;
	.

oe2020-crs-rec:CourseSchedule
	a owl:Class ;
	rdfs:subClassOf
		oe2020-crs-rec:SemesterSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasMeetingTime ;
			owl:onDataRange xsd:dateTime ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "course schedule" ;
	.

oe2020-crs-rec:CourseSection
	a owl:Class ;
	rdfs:subClassOf
		owl:Thing ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:courseInstanceOf ;
			owl:onClass oe2020-crs-rec:Course ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasSchedule ;
			owl:onClass oe2020-crs-rec:SemesterSchedule ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "course section" ;
	.

oe2020-crs-rec:Degree
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasRequirement ;
			owl:someValuesFrom oe2020-crs-rec:Requirement ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasDepartment> ;
			owl:someValuesFrom oe2020-crs-rec:Department ;
		]
		;
	rdfs:label "degree" ;
	.

oe2020-crs-rec:Department
	a owl:Class ;
	rdfs:label "department" ;
	.

oe2020-crs-rec:Discipline
	a owl:Class ;
	rdfs:label "discipline" ;
	.

oe2020-crs-rec:EducationalInstitution
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-fct-fct:FunctionalBusinessEntity ,
		fibo-be-oac-exec:AuthorizingParty ,
		fibo-fnd-pas-pas:ServiceProvider
		;
	rdfs:label "educational institution" ;
	skos:definition "institution where people gain an education, providing a variety of learning environments and spaces" ;
	fibo-fnd-utl-av:adaptedFrom
		"https://definitions.uslegal.com/e/educational-institution/"^^xsd:anyURI ,
		"https://www.thefreedictionary.com/educational+institution"^^xsd:anyURI
		;
	fibo-fnd-utl-av:explanatoryNote "According to 20 USCS section 1681 educational institution means 'any public or private preschool, elementary, or secondary school, or any institution of vocational, professional, or higher education, except that in the case of an educational institution composed of more than one school, college, or department which are administratively separate units, such term means each such school, college, or department.'" ;
	.

oe2020-crs-rec:FacultyMember
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-oac-exec:AuthorizedParty ,
		fibo-fnd-pty-pty:OrganizationMember ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty lcc-lr:isMemberOf ;
				owl:someValuesFrom [
					a owl:Restriction ;
					owl:onProperty fibo-fnd-pty-rl:playsRole ;
					owl:someValuesFrom oe2020-crs-rec:University ;
				] ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasDepartment> ;
			owl:someValuesFrom oe2020-crs-rec:Department ;
		]
		;
	rdfs:label "faculty member" ;
	skos:definition "party employed by a college or district to conduct classroom or teaching activities or who is otherwise considered by the college to be a member of faculty" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.lawinsider.com/dictionary/faculty-member"^^xsd:anyURI ;
	.

oe2020-crs-rec:Major
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty <https:/tw.rpi.edu/ontology-engineering/oe2020/20201003/course-recommender/hasDepartment> ;
		owl:someValuesFrom oe2020-crs-rec:Department ;
	] ;
	rdfs:label "major" ;
	.

oe2020-crs-rec:PlanOfStudy
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasClassYear ;
			owl:someValuesFrom xsd:dateTime ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasPlannedDegree ;
			owl:someValuesFrom oe2020-crs-rec:Degree ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasPlannedMajor ;
			owl:someValuesFrom oe2020-crs-rec:Department ;
		]
		;
	rdfs:label "plan of study" ;
	.

oe2020-crs-rec:Professor
	a owl:Class ;
	rdfs:subClassOf
		oe2020-crs-rec:FacultyMember ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:teaches ;
			owl:someValuesFrom oe2020-crs-rec:ScheduledCourseSection ;
		]
		;
	rdfs:label "professor" ;
	skos:definition "teacher of the highest rank in a college or university" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Professor"^^xsd:anyURI ;
	.

oe2020-crs-rec:RensselaerPolytechnicInstitute
	a
		owl:NamedIndividual ,
		oe2020-crs-rec:University
		;
	rdfs:label "Rensselaer Polytechnic Institute" ;
	skos:definition "globally recognized university situated on 275 acres in Troy NY, offering 746 labs, studios, and technology spaces used for research" ;
	fibo-fnd-rel-rel:hasIdentity oe2020-crs-rec:RensselaerPolytechnicInstituteAsLegalEntity ;
	fibo-fnd-utl-av:abbreviation
		"RPI" ,
		"Rensselaer"
		;
	fibo-fnd-utl-av:adaptedFrom "https://rpi.edu/about/"^^xsd:anyURI ;
	.

oe2020-crs-rec:RensselaerPolytechnicInstituteAddress
	a
		owl:NamedIndividual ,
		fibo-fnd-plc-adr:PhysicalAddress
		;
	rdfs:label "Rensselaer Polytechnic Institute address" ;
	skos:definition "physical street address for Rensselaer Polytechnic Institute" ;
	fibo-fnd-plc-adr:hasAddressLine1 "110 8th Street" ;
	fibo-fnd-plc-adr:hasPostalCode "12180-3522" ;
	fibo-fnd-plc-loc:hasCityName "Troy" ;
	fibo-fnd-plc-loc:hasCountry lcc-3166-1:UnitedStates ;
	fibo-fnd-plc-loc:hasSubdivision lcc-3166-2-us:NewYork ;
	.

oe2020-crs-rec:RensselaerPolytechnicInstituteAsLegalEntity
	a
		owl:NamedIndividual ,
		fibo-be-le-lp:LegalEntity
		;
	rdfs:label "Rensselaer Polytechnic Institute as legal entity" ;
	skos:definition "legal entity that is Rensselaer Polytechnic Institute" ;
	fibo-be-le-fbo:hasHeadquartersAddress oe2020-crs-rec:RensselaerPolytechnicInstituteAddress ;
	.

oe2020-crs-rec:Requirement
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasValidCourses ;
			owl:someValuesFrom oe2020-crs-rec:Course ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:requiresCredits ;
			owl:someValuesFrom xsd:int ;
		]
		;
	rdfs:label "requirement" ;
	.

oe2020-crs-rec:ScheduledCourseSection
	a owl:Class ;
	rdfs:subClassOf
		oe2020-crs-rec:CourseSection ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasInstructor ;
			owl:onClass oe2020-crs-rec:Professor ;
			owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasSchedule ;
			owl:onClass oe2020-crs-rec:CourseSchedule ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasCourseRegistrationNumber ;
			owl:onDataRange xsd:string ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasRegisteredStudent ;
			owl:someValuesFrom oe2020-crs-rec:Student ;
		]
		;
	rdfs:label "scheduled course section" ;
	.

oe2020-crs-rec:Semester
	a
		owl:NamedIndividual ,
		fibo-fnd-dt-fd:CalendarPeriod
		;
	rdfs:label "semester" ;
	skos:definition "18-week period of instruction into which an academic year is often divided" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.merriam-webster.com/dictionary/semester"^^xsd:anyURI ;
	.

oe2020-crs-rec:SemesterSchedule
	a owl:Class ;
	rdfs:subClassOf
		oe2020-crs-rec:AcademicSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasSemester ;
			owl:onDataRange xsd:dateTime ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "semester schedule" ;
	.

oe2020-crs-rec:Student
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-pty-pty:PartyInRole ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:onClass fibo-fnd-aap-ppl:Person ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:isRegisteredBy ;
			owl:someValuesFrom fibo-fbc-fct-ra:Registrar ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2020-crs-rec:hasAdvisor ;
			owl:someValuesFrom oe2020-crs-rec:Advisor ;
		] ,
